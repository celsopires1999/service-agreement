{
  "name": "service-agreement",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:studio": "env $(cat .env.drizzle | grep -v '^#' | xargs) drizzle-kit studio",
    "db:generate": "env $(cat .env.drizzle | grep -v '^#' | xargs) drizzle-kit generate",
    "db:push": "env $(cat .env.drizzle | grep -v '^#' | xargs) drizzle-kit push",
    "db:migrate": "env $(cat .env.drizzle | grep -v '^#' | xargs) tsx ./src/db/migrate.ts",
    "db:seed": "env $(cat .env.drizzle | grep -v '^#' | xargs) tsx ./src/db/seed.ts",
    "test": "jest --coverage",
    "test:core": "jest --config jest.config.core.ts",
    "test:ui": "jest --config jest.config.ui.ts",
    "test:watch": "jest --watch",
    "dev:test:e2e": "env $(cat .env.e2e | grep -v '^#' | xargs) next dev --turbopack",
    "test:e2e": "env $(cat .env.e2e | grep -v '^#' | xargs) playwright test",
    "test:e2e:update": "env $(cat .env.e2e | grep -v '^#' | xargs) playwright test --update-snapshots --reporter null",
    "test:e2e:headed": "env $(cat .env.e2e | grep -v '^#' | xargs) playwright test --headed --reporter null",
    "test:e2e:debug": "env $(cat .env.e2e | grep -v '^#' | xargs) playwright test --debug --reporter null",
    "test:e2e:ui": "env $(cat .env.e2e | grep -v '^#' | xargs) playwright test --ui-host=0.0.0.0 --reporter null",
    "test:e2e:report": "env $(cat .env.e2e | grep -v '^#' | xargs) playwright show-report",
    "deploy": "git pull && npm ci && next build && pm2 restart service-agreement"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "decimal.js": "^10.5.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "next": "^15.1.7",
    "next-auth": "^5.0.0-beta.27",
    "next-safe-action": "^7.10.4",
    "next-themes": "^0.4.4",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "xlsx": "file:vendor/xlsx-0.20.3.tgz",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.52.0",
    "@testcontainers/postgresql": "^11.5.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chance": "^1.1.7",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "chance": "^1.1.13",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.20.1",
    "eslint-config-next": "^15.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}